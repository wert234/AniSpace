<UserControl x:Class="AniSpace.Infructuctre.UserControls.AnimeGaner.AnimeGaner"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AniSpace.Infructuctre.UserControls.AnimeGaner"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Name="Ganer">
    <CheckBox
  Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"
  Content="{Binding ElementName=Ganer, Path=GanerName}"
        IsChecked="{Binding ElementName=Ganer, Path=GanerSelected}"
        HorizontalAlignment="Center" Cursor="Hand" />
</UserControl>
